  type: "ROIPooling"
  bottom: "conv5_3"
  bottom: "sppboxes"
  top: "pool5"
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
  }
}
layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "pool5"
  top: "fc6"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  inner_product_param {
