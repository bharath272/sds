# ####Hypercolumn layers####
layer {
  name: "scale_pool3"
  type: "Scale"
  bottom: "pool3"
  top: "spool3"
  scale_param {
    scale: 0.0027
  }
}
layer {
  name: "scale_pool4"
  type: "Scale"
  bottom: "pool4"
  top: "spool4"
  scale_param {
    scale: 0.063
  }
}
layer {
  name: "conv_pool3" 
  type: "Convolution"
  bottom: "spool3"
  top: "conv_pool3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 500
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "conv_pool4" 
  type: "Convolution"
  bottom: "spool4"
  top: "conv_pool4"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 500
    pad: 0
    kernel_size: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "fcfc7" 
  type: "InnerProduct"
  bottom: "fc7"
  top: "fcfc7"
  param {
    lr_mult: 0.1
    decay_mult: 0.1
  }
  param {
    lr_mult: 0.2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 500
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "ups_pool3"
  type: "BoxUpsample"
  bottom: "conv_pool3"
  bottom: "normalizedboxes"
  top: "ups_pool3"
  box_upsample_param {
    width: 50
    height: 50
    channels: 25
  }
}
layer {
  name: "ups_pool4"
  type: "BoxUpsample"
  bottom: "conv_pool4"
  bottom: "normalizedboxes"
  top: "ups_pool4"
  box_upsample_param {
    width: 50
    height: 50
    channels: 25
  }
}
layer {
  name: "slice_fc7"
  type: "PickSlice"
  bottom: "fcfc7"
  bottom: "categids"
  top: "fcfc7out"
  pick_slice_param {
    channels: 25
  }
}
layer {
  name: "repmat"
  type: "RepMat"
  bottom: "fcfc7out"
  top: "ups_fc7"
  repmat_param{
    rep_n: 1
    rep_c: 1
    rep_w: 50
    rep_h: 50
  }
}
layer {
  name: "sum"
  type: "Sum"
  bottom: "ups_pool3"
  bottom: "ups_pool4"
  bottom: "ups_fc7"
  top: "sum"
}
layer {
  name: "grid"
  type: "LocallyConnectedWithLoss"
  bottom: "sum"
  bottom: "labels"
  bottom: "instance_wts"
  top: "loss"
  local_layer_param {
    grid_size: 5
    pos_loss_wt: 1
    neg_loss_wt: 1
  }
  loss_weight: 1
}

