name: "try"
layer {
  name: "data"
  type: "Python"
  top: "image"
  top: "normalizedboxes"
  top: "sppboxes"
  top: "categids"
  top: "labels"
  top: "instance_wts"
  python_param {
    module: "hypercolumn_data_layer"
    layer: "HypercolumnDataLayer"
    param_str: '--proposalfile train_mcg_boxes.pkl --gtfile gttrain.pkl'
  }
}
